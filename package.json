{
  "name": "introspection-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts --max_old_space_size=8192 start",
    "build": "react-scripts --max_old_space_size=8192 build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --no-watch",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:once",
      "pre-push": "npm run test:once",
      "post-merge": "npm install && npm run test:once"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "i": "^0.3.6",
    "lodash": "^4.17.11",
    "npm": "^6.9.0",
    "plotly.js": "^1.47.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-plotly.js": "^2.3.0",
    "react-router-dom": "^5.0.0",
    "react-s-alert": "^1.4.1",
    "react-scripts": "2.1.8",
    "reactstrap": "^8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "history": "^4.9.0",
    "husky": "^1.3.1",
    "jest-dom": "^3.1.3",
    "jest-fetch-mock": "^2.1.2",
    "react-testing-library": "^6.1.2"
  }
}
