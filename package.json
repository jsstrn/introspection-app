{
  "name": "introspection-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts --max_old_space_size=8192 start",
    "start:dev": "concurrently 'npm:start' 'npm:mock-server'",
    "build": "react-scripts --max_old_space_size=8192 build",
    "test": "react-scripts test",
    "test:once": "react-scripts test --no-watch",
    "test:ci": "CI=true react-scripts test",
    "test:all": "npm run test:once && npm run cy:run",
    "eject": "react-scripts eject",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "npm start & npm run cy:run",
    "mock-server": "json-server mockServer.json --port 7890"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:all"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "bytes": "^3.1.0",
    "lodash": "^4.17.11",
    "npm": "^6.9.0",
    "plotly.js": "^1.47.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.4",
    "react-plotly.js": "^2.3.0",
    "react-router-dom": "^5.0.0",
    "react-s-alert": "^1.4.1",
    "react-scripts": "2.1.8",
    "reactstrap": "^8.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cypress": "^3.2.0",
    "eslint-plugin-cypress": "^2.2.1",
    "history": "^4.9.0",
    "husky": "^1.3.1",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "jest-dom": "^3.1.3",
    "jest-fetch-mock": "^2.1.2",
    "json-server": "^0.14.2",
    "react-testing-library": "^6.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
